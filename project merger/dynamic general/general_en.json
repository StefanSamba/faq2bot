if(Array.isArray(payload.params.bericht)) {\n var bericht = payload.params.bericht[0].value\n  }\n\n\n  toolbelt.email({\n    to: 'Support Email', // <-- Vul hier een email adres in\n    subject: 'Nieuwe vraag via de chatbot',\n    message: `naam: ${naam}, <br/>\n              email: ${email}, <br/>\n              bericht: ${bericht}`\n  })\n}","description":"Cloud code","eventTypes":null,"group":"Untitled group","integrationId":"55361786-a958-43d9-a777-b4479436781b"}]}